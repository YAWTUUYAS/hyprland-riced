[
    {
        "label": "lock",
        "action": "hyprlock",
        "keybind": "l"
    },
    {
        "label": "reboot",
        "action": "systemctl reboot",
        "keybind": "r"
    },
    {
        "label": "logout",
        "action": "hyprctl dispatch exit 0",
        "keybind": "e"
    },
    {
        "label": "shutdown",
        "action": "systemctl poweroff",
        "keybind": "s"
    }
]

